!!!
html
  head
    title= title
    link(href='assets/styles.css', rel='stylesheet')
    script(src='assets/jquery-1.7.1.js', type='text/javascript')
    script(src='assets/script.js', type='text/javascript')
  body
    h1 NodObjC
    h2= title
    #content
      each part in input
        if (!part.ignore)
          if (part.ctx && part.ctx.type == 'method')
            h3= part.ctx.string
            .part
              .desc!= part.description.full
              if (part.code)
                button.show-code Show Code
                pre.code
                  code= part.code
